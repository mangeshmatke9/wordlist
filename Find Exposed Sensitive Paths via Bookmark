javascript:(async function(){  const listUrl = prompt("Enter the RAW URL of your wordlist:");  if (!listUrl) return;  const panel = document.createElement("div");  Object.assign(panel.style, {    position:"fixed",    bottom:"0",    left:"0",    width:"100%",    maxHeight:"40%",    overflowY:"auto",    background:"#fff",    color:"#000",    borderTop:"2px solid #000",    padding:"10px",    zIndex:999999  });  panel.innerHTML="<h3>Scanning…</h3>";  document.body.appendChild(panel);  let res;  try {    res = await fetch(listUrl);  } catch (e) {    alert("Could not load the wordlist URL.");    return;  }  const paths = (await res.text()).split("\n").map(x=>x.trim()).filter(Boolean);  const found = [];  for (const p of paths) {    try {      const r = await fetch(p);      if (r.ok) {        found.push(p);        const row = document.createElement("div");        row.style.color="red";        row.textContent = %60⚠%EF%B8%8F FOUND: ${p} (status ${r.status})%60;        panel.appendChild(row);        console.log("FOUND:", p, r.status);      }    } catch (e) {}  }  panel.innerHTML += %60<br><strong>Scan complete — ${found.length} exposed paths found.</strong>%60;  console.table(found);})();
